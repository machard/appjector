{
  "name": "appjector",
  "version": "1.0.1",
  "description": "Appjector is a simple lib that aims to simplify dependencies management and to ease testing for node projects",
  "main": "index.js",
  "scripts": {
    "test": "grunt test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/machard/appjector.git"
  },
  "config": {
    "travis-cov": {
      "threshold": 100
    },
    "blanket": {
      "pattern": [
        "src"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "author": "Matthieu Achard <matthieu__@hotmail.fr>",
  "license": "MIT",
  "devDependencies": {
    "grunt": "0.4.5",
    "grunt-contrib-clean": "0.6.0",
    "grunt-contrib-jshint": "0.10.0",
    "grunt-contrib-watch": "0.6.1",
    "grunt-gh-pages": "0.9.1",
    "grunt-jsdoc": "0.5.7",
    "grunt-mocha-cov": "0.3.0",
    "jaguarjs-jsdoc": "machard/jaguarjs-jsdoc",
    "matchdep": "0.3.0",
    "sinon": "1.12.2",
    "travis-cov": "0.2.5"
  },
  "dependencies": {
    "fn-params": "0.0.1",
    "lodash": "2.4.1"
  }
}
